{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@utils/*": ["src/utils/*"],
			"@icons/*": ["src/icons/*"],
			"@hooks/*": ["src/hooks/*"],
			"@stores/*": ["src/stores/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
